{
  "name": "articles_nlp",
  "version": "1.0.0",
  "description": "A web tool that allows users to run Natural Language Processing (NLP) on articles or blogs found on other websites.",
  "main": "index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "dev": "nodemon src/server/index.js",
    "build-dev": "node --openssl-legacy-provider ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.dev.js",
    "build-prod": "rm -rf dist && node --openssl-legacy-provider ./node_modules/webpack/bin/webpack.js --config webpack.prod.js",
    "test": "jest"
  },
  "author": "Yousef Saeed",
  "dependencies": {
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "sass": "^1.35.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^5.2.6",
    "html-webpack-plugin": "^4.5.2",
    "jest": "^27.0.6",
    "mini-css-extract-plugin": "^1.3.3",
    "nodemon": "^2.0.9",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "sass-loader": "^10.0.0",
    "style-loader": "^2.0.0",
    "supertest": "^6.1.3",
    "terser-webpack-plugin": "^4.1.0",
    "webpack-dev-server": "^3.7.2",
    "workbox-webpack-plugin": "^6.1.5"
  }
}
